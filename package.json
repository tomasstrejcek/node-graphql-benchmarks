    "express-graphql": "^0.12.0",
    "fast-json-stringify": "^3.0.3",
    "fastify": "^3.11.0",
    "graphql": "^15.8.0",
    "graphql-api-koa": "^8.0.0",
    "graphql-compose": "^9.0.8",
