    "apollo-server-fastify": "^3.6.7",
    "apollo-server-koa": "^3.6.7",
    "autocannon": "^7.0.3",
    "autocannon-compare": "^0.4.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.4",
